class Solution:
    def kidsWithCandies(self, candies: List[int], extraCandies: int) -> List[bool]:
        i=0
        #i=max(candies)
        for z in candies:
            if z>i:
                i=z
        
        result=[]
        
        for x in candies:
            if x+extraCandies>=i:
                result.append(True)
            else:
                result.append(False)
        return result 
